<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.leadu.fms.data.system.dao.SysRoleMenuDao">

    <delete id="deleteRoleMenusByRoleId" parameterType="java.lang.String" flushCache="true">

        update sys_role_menu set del_flag = ${delete_not_exist}  where role_id = #{roleId}

    </delete>

</mapper>